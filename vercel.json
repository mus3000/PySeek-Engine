{
    "builds" : [
        {
            "src" : "api.py",
            "use" : "@vercel/python"
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "/api.py"
        }
    ],
    "functions": {
        "api/**": {
            "excludeFiles": "{__pycache__,myenv}/**"
        }
    }
}