{
    "builds" : [
        {
            "src" : "api.py",
            "use" : "@vercel/python",
            "excludeFiles": [
                "__pyacahce__/**",
                "myenv/**"
            ]
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "/api.py"
        }
    ]
}